}

model ConnectorTypes {
  ID      Int     @id(map: "ConnectorTypes_PK") @default(autoincrement())
  Type    String  @db.VarChar(100)
  Section String? @db.VarChar(100)
}

model ContagemNumVias {
  QtdVias      Int    @unique(map: "ContagemNumVias_UNIQUE")
  ContagemVias String @db.VarChar(100)
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model Cores {
