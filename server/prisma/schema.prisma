generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model AccessoryTypes {
  ID              Int    @unique(map: "AccessoryTypes_UNIQUE") @default(autoincrement())
  TypeDescription String @db.VarChar(100)
}

